{
  "manifest_version": 3,
  "name": "Nom Nom",
  "version": "1.0",
  "description": "Remove unwanted domains from Google search results ðŸŽ®",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/*",
        "*://google.com/*",
        "*://www.google.de/*",
        "*://google.de/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
