{
  "manifest_version": 3,
  "name": "Nom Nom",
  "version": "1.0",
  "description": "Remove unwanted domains from Google search results ðŸŽ®",
  "host_permissions": [
    "*://*.google.at/*",
    "*://*.google.be/*",
    "*://*.google.ca/*",
    "*://*.google.ch/*",
    "*://*.google.co.in/*",
    "*://*.google.co.jp/*",
    "*://*.google.co.uk/*",
    "*://*.google.com/*",
    "*://*.google.com.au/*",
    "*://*.google.com.br/*",
    "*://*.google.cz/*",
    "*://*.google.de/*",
    "*://*.google.dk/*",
    "*://*.google.es/*",
    "*://*.google.fi/*",
    "*://*.google.fr/*",
    "*://*.google.ie/*",
    "*://*.google.it/*",
    "*://*.google.nl/*",
    "*://*.google.no/*",
    "*://*.google.pl/*",
    "*://*.google.pt/*",
    "*://*.google.ru/*",
    "*://*.google.se/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.google.at/*",
        "*://*.google.be/*",
        "*://*.google.ca/*",
        "*://*.google.ch/*",
        "*://*.google.co.in/*",
        "*://*.google.co.jp/*",
        "*://*.google.co.uk/*",
        "*://*.google.com/*",
        "*://*.google.com.au/*",
        "*://*.google.com.br/*",
        "*://*.google.cz/*",
        "*://*.google.de/*",
        "*://*.google.dk/*",
        "*://*.google.es/*",
        "*://*.google.fi/*",
        "*://*.google.fr/*",
        "*://*.google.ie/*",
        "*://*.google.it/*",
        "*://*.google.nl/*",
        "*://*.google.no/*",
        "*://*.google.pl/*",
        "*://*.google.pt/*",
        "*://*.google.ru/*",
        "*://*.google.se/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
